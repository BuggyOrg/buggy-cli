{
  "name": "@buggyorg/buggy",
  "version": "0.1.0",
  "description": "Rich code generation system.",
  "main": "api.js",
  "bin": {
    "buggy": "cli.js"
  },
  "scripts": {
    "test": "node node_modules/standard/bin/cmd.js src/**/*.js && babel src -d lib && node_modules/mocha/bin/mocha --compilers js:babel/register",
    "build": "babel src -d lib",
    "prepublish": "babel src -d lib"
  },
  "keywords": [
    "Code-Generation"
  ],
  "author": "Maximilian Klein",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "@buggyorg/component-library": "^0.3.1",
    "@buggyorg/dupjoin": "^0.1.4",
    "@buggyorg/dynatype-network-graph": "^0.1.4",
    "@buggyorg/functional": "^0.1.0",
    "@buggyorg/gogen": "^0.1.4",
    "@buggyorg/graphify": "^0.1.7",
    "@buggyorg/graphlib2kgraph": "^0.1.2",
    "@buggyorg/npg-port-remodeler": "^0.1.8",
    "@buggyorg/resolve": "^0.1.9",
    "@buggyorg/typify": "^0.1.0",
    "babel-polyfill": "^6.7.4",
    "commander": "^2.9.0",
    "get-stdin": "^5.0.1",
    "graphlib": "^2.1.0",
    "promised-exec": "^1.0.1",
    "tempfile": "^1.1.1"
  }
}
